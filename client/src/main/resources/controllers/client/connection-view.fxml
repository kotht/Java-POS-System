<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="350.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.client.ClientController">
   <children>
      <Label fx:id="lblConnectError" layoutX="50.0" layoutY="88.0" prefHeight="17.0" prefWidth="250.0" textFill="#ff3300f2" />
      <TextField fx:id="tfAddress" layoutX="50.0" layoutY="111.0" prefHeight="25.0" prefWidth="250.0" promptText="Enter Address Here" />
      <Button layoutX="50.0" layoutY="150.0" mnemonicParsing="false" onAction="#connect" prefHeight="25.0" prefWidth="250.0" text="Connect" />
   </children>
</AnchorPane>
